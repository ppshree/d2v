{
  "name": "platform-v2",
  "version": "0.1.0",
  "private": true,
  "author": "Swayam P Rout (3swayam@gmail.com)",
  "dependencies": {
    "-": "0.0.1",
    "@material-ui/core": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@reduxjs/toolkit": "^1.1.0",
    "@sentry/react": "^6.1.0",
    "D": "^1.0.0",
    "axios": "^0.21.0",
    "chart.js": "^2.9.4",
    "env-cmd": "^10.1.0",
    "i18next": "^19.9.1",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "postcss": "^8.2.15",
    "query-string": "^6.12.1",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.8",
    "react-moment": "^1.1.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "use-query-params": "^1.1.3"
  },
  "scripts": {
    "build:css": "postcss src/styles/talwind/talwind.css -o src/styles/output/output.css",
    "watch:css": "postcss src/styles/talwind/talwind.css -o src/styles/output/output.css -w",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write \"**/*.+(js)\"",
    "server:dev": "env-cmd -f .env.dev react-scripts start",
    "server:stage": "env-cmd -f .env.staging react-scripts start",
    "server:prod": "env-cmd -f .env.prod react-scripts start",
    "production:dev": "env-cmd -f .env.dev react-scripts build",
    "production:stage": "env-cmd -f .env.staging react-scripts build",
    "production:prod": "env-cmd -f .env.prod react-scripts build",
    "start:dev": "npm-run-all -p watch:css server:dev",
    "start:stage": "npm-run-all -p watch:css server:stage",
    "start:prod": "npm-run-all -p watch:css server:prod",
    "build:dev": "npm-run-all -p watch:css production:dev",
    "build:stage": "npm-run-all -p watch:css production:stage",
    "build:prod": "npm-run-all -p watch:css production:prod"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "addon-redux": "^1.1.0",
    "autoprefixer": "^10.2.5",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.3"
  }
}
